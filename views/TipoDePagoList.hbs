<h1>Tipos De Pago</h1>

<a href="/paymentType/create" class="btn btn-primary">Nuevo</a>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Tipo De Pago</th>
            <th scope="col">Activo</th>
            <th scope="col">Acciones</th>
        </tr>
    </thead>
    <tbody>
        {{#each paymentType}}
        <tr>
            <td scope="row">{{ID}}</td>
            <td>{{PaymentType}}</td>
            <td>{{Activo}}</td>
            <td>
                <div>
                    <a href="/paymentType/view/{{ID}}" class="btn btn-primary">Ver</a>
                    <a href="/paymentType/edit/{{ID}}" class="btn btn-warning">Editar</a>
                    <button type="button" class="btn btn-danger">Eliminar</button>
                </div>
            </td>

        </tr>
        {{/each}}

    </tbody>
</table>